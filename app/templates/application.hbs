<!-- Application.hbs contains everything that should display on any page of our application. In this case, it contains a navigation bar -->
<div class="container">
  <div class="menu">
    {{#link-to "index"}}
      <h1>
        <em>SuperRentals</em>
      </h1>
    {{/link-to}}
    <div class="links">
      {{#link-to "about"}}
        About
      {{/link-to}}
      {{#link-to "contact"}}
        Contact
      {{/link-to}}
    </div>
  </div>
  <div class="body">
	<!-- Outlet is a helper that tells ember to render whatever route we're on here -->
    {{outlet}}
  </div>
</div>